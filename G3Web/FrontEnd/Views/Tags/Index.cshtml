@model IEnumerable<GamesTore.Models.GetTagDTO>

@{
    ViewBag.Title = "Index";
}


<table class="table">
    <tr>
        
        <th>
            @Html.ActionLink("Tag Name", "Index")
        </th>
        <th>@Html.ActionLink("Action", "Index")</th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { URL = item.URL }) |
            @Html.ActionLink("Details", "Details", new { URL = item.URL }) |
            @Html.ActionLink("Delete", "Delete", new { URL = item.URL })
        </td>
    </tr>
}

</table>
